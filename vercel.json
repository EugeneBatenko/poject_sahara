{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "app.js",
      "methods": ["GET", "POST"],
      "continue": true
    },
    { "src": "/(.+js|.+css|.+png|.+ico|robots.txt)", "dest": "/public/$1" }
  ]
}
